<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <SolidColorBrush x:Key="NavigationViewContentBackground" Color="Transparent" />

    <system:Boolean x:Key="Boolean.True">true</system:Boolean>
    <system:Boolean x:Key="Boolean.False">false</system:Boolean>

    <BitmapImage x:Key="LogoBitmapImage" UriSource="pack://application:,,,/StudyHub.WPF;component/Resources/Images/logo.png" />
    <BitmapImage x:Key="PencilsBitmapImage" UriSource="pack://application:,,,/StudyHub.WPF;component/Resources/Images/pencils.png" />

    <system:String x:Key="ApplicationTitle">StudyHub</system:String>

</ResourceDictionary>
